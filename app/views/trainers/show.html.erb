<h3>Trainer</h3>
<p>Name: <%= @trainer.name if @trainer.name %></p>
<p>Email: <%= @trainer.email if @trainer.email %></p>
<h4>Pokemon</h4>	
<p> Name Level Health </p>
<% @pokemons.each do |p| %>
  <div>
  <%= p.name %>
  <%= p.level %>
  <%= p.health %>
  <%= button_to "DAMAGE!", capture_path(id: @pokemon), :class => "button medium", :method => :patch %>
  </div>
<% end %>


